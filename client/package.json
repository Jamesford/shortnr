{
  "name": "AppReact",
  "version": "0.2.0",
  "description": "A Flux + React App",
  "author": "Jamesford",
  "license": "MIT",
  "dependencies": {
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "moment": "^2.9.0",
    "object-assign": "^1.0.0",
    "q": "^1.2.0",
    "react": "^0.12.0",
    "react-router": "^0.12.0",
    "superagent": "^1.1.0"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "main": "js/app.js",
  "scripts": {
    "start": "watchify -o dist/bundle.js -v -d js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > dist/bundle.min.js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
